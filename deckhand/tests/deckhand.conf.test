[DEFAULT]
debug = true
publish_errors = true
use_stderr = true
development_mode = false

[oslo_policy]
policy_file = policy.yaml

[barbican]

[database]
connection = ${AIRSHIP_DECKHAND_DATABASE_URL}

[engine]
enable_cache = false

[keystone_authtoken]
# NOTE(fmontei): Values taken from clouds.yaml. Values only used for
# integration testing.
#
# clouds.yaml (snippet):
#
# username: 'admin'
# password: 'password'
# project_name: 'admin'
# project_domain_name: 'default'
# user_domain_name: 'default'
# auth_url: 'http://keystone.openstack.svc.cluster.local/v3'

username = admin
password = password
project_name = admin
project_domain_name = Default
user_domain_name = Default
auth_url = http://keystone.openstack.svc.cluster.local/v3
auth_type = password
